programa
{

	funcao inicio ()
	{

		inteiro A,B,C
		escreva ("Digite o Valor de A: ")
		leia (A)
		escreva ("Digite o valor de B: ")
		leia (B)
		escreva ("Digite o valor de C: ")
		leia (C)
		se ((A==B) e (A==C)e (B==C))
		{escreva("\nEquivale a um triangulo Equilatero")}
			senao se ((A==B) ou (A==C) ou (B==C))
		{escreva("\nEquivale a um triangulo Isosceles ")}
				senao 
				escreva ("\nEquivale a um triangulo escaleno ")
			
			
		
		
		
		
		
		
		
		
		
	}
}